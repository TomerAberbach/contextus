{
  "name": "contextus",
  "version": "1.1.0",
  "author": {
    "name": "Tomer Aberbach",
    "email": "tomer@aberba.ch",
    "url": "https://tomeraberba.ch"
  },
  "description": "The context you know and love, but framework agnostic!",
  "keywords": [
    "context",
    "react",
    "react-context",
    "state",
    "props",
    "dependency injection",
    "provider",
    "provide",
    "use"
  ],
  "homepage": "https://github.com/TomerAberbach/contextus",
  "repository": "TomerAberbach/contextus",
  "bugs": {
    "url": "https://github.com/TomerAberbach/contextus/issues"
  },
  "license": "Apache-2.0",
  "files": [
    "dist",
    "src/index.d.ts"
  ],
  "type": "module",
  "engines": {
    "node": ">= 12.17"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not op_mini all"
  ],
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./src/index.d.ts",
  "scripts": {
    "prepare": "tomer install",
    "lint": "tomer lint -g",
    "test": "tomer test",
    "build": "tomer build -f \"esm,cjs\" src/index.js",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "ava": "3.15.0",
    "ava-fast-check": "4.0.2",
    "fast-check": "2.17.0",
    "tomer": "0.0.16",
    "tsd": "0.17.0"
  }
}
